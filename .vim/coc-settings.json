{
  "coc.preferences.rootPatterns": [],
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.maxWindowHeight": 24,
  "signature.maxWindowHeight": 24,
  "list.maxHeight": 24,
  "list.source.symbols.defaultOptions": ["--auto-preview"],
  "elixirLS.dialyzerEnabled": false,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "elixir.trace.server": "verbose",
  "__comment__: javascript.validate.enable": "Prevents flowjs errors from tsserver. Temporarily changed back to true but should be false",
  "javascript.validate.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.fixOnSaveTimeout": 500,
  "snippets.ultisnips.pythonPrompt": false
}
